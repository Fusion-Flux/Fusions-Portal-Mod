{
  "multipart": [
    {
      "when": {
        "ns": "true",
        "half": "lower"
      },
      "apply": {
        "model": "portalcubed:block/old_aperture_laser_field_bottom"
      }
    },
    {
      "when": {
        "ns": "true",
        "half": "upper"
      },
      "apply": {
        "model": "portalcubed:block/old_aperture_laser_field_top"
      }
    },
    {
      "when": {
        "ew": "true",
        "half": "lower"
      },
      "apply": {
        "model": "portalcubed:block/old_aperture_laser_field_bottom",
        "y": 90
      }
    },
    {
      "when": {
        "ew": "true",
        "half": "upper"
      },
      "apply": {
        "model": "portalcubed:block/old_aperture_laser_field_top",
        "y": 90
      }
    }
  ]
}